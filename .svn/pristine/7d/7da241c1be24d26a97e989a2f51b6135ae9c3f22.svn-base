<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dx.dxmanage.dao.SalesMapper">
	<resultMap id="BaseResultMap" type="com.dx.dxmanage.po.Sales">
		<id column="salesid" jdbcType="INTEGER" property="salesid" />
		<result column="salesdate" jdbcType="VARCHAR"
			property="salesdate" />
		<result column="goodsid" jdbcType="INTEGER" property="goodsid" />
		<result column="quantity" jdbcType="INTEGER"
			property="quantity" />
		<result column="payment" jdbcType="VARCHAR" property="payment" />
		<result column="customerid" jdbcType="INTEGER"
			property="customerid" />
		<result column="timelimit" jdbcType="VARCHAR"
			property="timelimit" />
		<result column="amount" jdbcType="DOUBLE" property="amount" />
		<result column="receipt" jdbcType="VARCHAR" property="receipt" />
		<result column="wares" jdbcType="VARCHAR" property="wares" />
		<result column="payeebank" jdbcType="VARCHAR"
			property="payeebank" />
		<result column="note" jdbcType="VARCHAR" property="note" />
		<result column="ticketnum" jdbcType="VARCHAR"
			property="ticketnum" />
		<result column="buyyear" jdbcType="VARCHAR" property="buyyear" />
		<result column="linkman" jdbcType="VARCHAR" property="linkman" />
		<result column="tel" jdbcType="VARCHAR" property="tel" />
		<result column="email" jdbcType="VARCHAR" property="email" />
		<result column="paycredentials" jdbcType="VARCHAR"
			property="paycredentials" />
		<collection property="goods" column="goodsid"
			select='com.dx.dxmanage.dao.GoodsMapper.selectByPrimaryKey'>
		</collection>
		<collection property="customer" column="customerid"
			select='com.dx.dxmanage.dao.CustomerMapper.selectByCustomerid'>
		</collection>

	</resultMap>
	<!--此mapper开启二级缓存 -->
	<!-- <cache /> -->
	<sql id="Base_Column_List">
		salesid, salesdate, goodsid, quantity, payment,
		customerid,
		timelimit, amount,
		receipt,
		wares,
		payeebank, note, ticketnum, buyyear,
		linkman, tel, email,
		paycredentials
	</sql>
	<sql id="Base_Column_List2">
		sales.salesid, salesdate, goodsid, quantity, payment,
		sales.customerid,
		sales.timelimit, amount,
		receipt,
		wares,
		payeebank,
		note, ticketnum, buyyear,
		linkman, tel, sales.email,
		paycredentials
	</sql>

	<select id="selectByPrimaryKey"
		parameterType="java.lang.Integer" resultMap="BaseResultMap">
		select
		<include refid="Base_Column_List" />
		from salesinfo
		where salesid = #{salesid,jdbcType=INTEGER}
	</select>
	<!-- 查询未开票的销售记录 -->
	<select id="selectNotTicketSales" resultMap="BaseResultMap">
		select
		<include refid="Base_Column_List" />
		from salesinfo sales
		where
		ticketnum = ''
		order by
		salesdate desc
	</select>

	<!-- 查询所有交易信息 -->
	<select id="selectAllSales" parameterType="Map"
		resultMap="BaseResultMap">
		select
		<include refid="Base_Column_List2" />
		from
		salesinfo sales
		left join spinfo sp on sales.salesid
		= sp.salesid
		left join customerinfo cus on sales.customerid
		= cus.customerid
		<where>
			1=1
			<if test="haveTicket==true">
				and sales.ticketnum !=''
			</if>
			<if test="customerid!=null">
				and sales.customerid = #{customerid}
			</if>
			<if test="notGetMoney==true">
				and sp.salesid is NULL
			</if>
			<if test="membership!='' and membership != null">
				and cus.membership = #{membership,jdbcType=VARCHAR}
			</if>
			<if test="codeOrName!='' and codeOrName!=null">
				and (cus.customercode like #{codeOrName}"%"
				or
				cus.customername
				like "%"#{codeOrName}"%")
			</if>

		</where>
		order by salesdate desc
	</select>
	<!-- 通过销售id删除销售信息 -->
	<delete id="deleteByPrimaryKey"
		parameterType="java.lang.Integer">
		delete from
		salesinfo
		where salesid =
		#{salesid,jdbcType=INTEGER}
	</delete>
	<!-- 增添销售信息 -->
	<insert id="addSales" parameterType="com.dx.dxmanage.po.Sales">
