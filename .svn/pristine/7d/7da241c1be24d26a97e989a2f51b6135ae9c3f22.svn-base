			</if>
		</trim>
	</insert>
	<!-- 更新销售信息 -->
	<update id="updateSales"
		parameterType="com.dx.dxmanage.po.Sales">
		update salesinfo
		<set>
			<if test="salesdate != null">
				salesdate = #{salesdate,jdbcType=VARCHAR},
			</if>
			<if test="goodsid != null">
